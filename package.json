{
  "name": "book-app",
  "version": "1.0.0",
  "description": "A book trading app from the fcc fullstack challenges",
  "main": "index.js",
  "scripts": {
    "start": "node build/server/backend.js",
    "start:dev": "concurrently \"npm run start:mongodb\"  \"npm run start:client\" \"npm run start:server\"",
    "start:client": "nodemon --watch webpack.config.js --watch config --exec \"webpack-dev-server --env development\"",
    "start:server": "nodemon --watch app --watch server.js --watch models --exec \"webpack --env development&& npm start\"",
    "start:mongodb": "node config/startdb.js",
    "build:client": "webpack --env production",
    "build:server": "webpack --env production",
    "build": "npm run build:client && npm run build:server"
  },
  "keywords": [
    "book",
    "trading",
    "mern",
    "fcc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkoN95/Book-App.git"
  },
  "author": "MarkoN95",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.4.0",
    "connect-logger": "0.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "npm-install-webpack-plugin": "^4.0.4",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "pug-loader": "^2.3.0",
    "purifycss-webpack": "^0.4.2",
